    "eslint-plugin-promise": "^4.3.1",
    "git-commit-msg-linter": "^3.0.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "sucrase": "^3.17.1",
